<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/colorPrimaryDark"
    android:layout_height="match_parent"
    tools:context=".MainGuitarEffect">

    <TextView
        android:id="@+id/guitarTextTitle"
        android:layout_width="match_parent"
        android:gravity="center"
        android:textColor="@color/colorWhite"
        android:layout_marginTop="20dp"
        android:textSize="30dp"
        android:layout_height="wrap_content" android:text="Guitar Effects" app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:background="@color/colorPrimary"
        app:layout_constraintTop_toBottomOf="@+id/guitarTextTitle"
        app:layout_constraintStart_toStartOf="@+id/guitarTextTitle"
        app:layout_constraintEnd_toEndOf="@id/guitarTextTitle"
        android:layout_margin="20dp"
        />




</androidx.constraintlayout.widget.ConstraintLayout>